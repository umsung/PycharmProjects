[{
		"config": {
			"name": "logincase",
			"variables": {}
		}
	},
	{
		"test": {
			"name": "login case1",
			"request": {
				"url": "http://127.0.0.1:8000/api/v1/login/",
				"method": "POST",
				"headers": {
					"Content-Type": "application/json",
					"User-Agent": "python-requests/2.18.4"
				},
				"json": {
					"username": "test",
					"password": "123456"
				}
			},
            "extract":  [{
                "getValue": ""
            }],
			"validate": [{
					"eq": ["status_code", 200]
				},
				{
					"eq": ["headers.Content-Type", "application/json"]
				},
				{
					"eq": ["content.msg", "login success!"]
				}, {
					"eq": ["content.code", 0]

				}
			]
		}
	}
]